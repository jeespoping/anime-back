{
  "kind": "collectionType",
  "collectionName": "animes",
  "info": {
    "singularName": "anime",
    "pluralName": "animes",
    "displayName": "anime",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "summer": {
      "type": "text",
      "required": true
    },
    "url": {
      "type": "uid",
      "targetField": "title"
    },
    "poster": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images"
      ]
    },
    "screenshots": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images"
      ]
    },
    "estado": {
      "type": "boolean",
      "default": true
    },
    "chapters": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::chapter.chapter",
      "mappedBy": "anime"
    },
    "types": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::type.type",
      "mappedBy": "anime"
    },
    "seasons": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::season.season",
      "mappedBy": "anime"
    }
  }
}
